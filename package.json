{
  "name": "af-tools-ts",
  "version": "1.1.4",
  "description": "tools",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "author": "Viacheslav Makarov <vvmakarov@bazilio.ru>",
  "license": "ISC",
  "keywords": ["tools"],
  "productName": "AF tools (ts)",
  "scripts": {
    "clean": "shx rm -rf dist/*",
    "build": "tsc -b tsconfig.json tsconfig.esm.json tsconfig.types.json",
    "lint": "eslint . --ext .ts ",
    "lint:fix": "eslint . --ext .ts --fix",
    "test-config-info": "node ./dist/cjs/__tests__/config-info.js",
    "test-date-utils": "node ./dist/cjs/__tests__/date-utils.js",
    "test-crypto-hash-random": "node ./dist/cjs/__tests__/crypto-hash-random.js",
    "pub": "npm run lint:fix && npm run clean && npm run build && npm publish",
    "cb": "npm run clean && npm run build"
  },
  "dependencies": {
    "@types/luxon": "^3.3.3",
    "af-color": "^0.0.8",
    "af-echo-ts": "^1.0.12",
    "config": "^3.3.9",
    "dotenv": "^16.3.1",
    "humanize-duration": "^3.30.0",
    "json-to-pretty-yaml": "^1.2.2",
    "luxon": "^3.4.3"
  },
  "devDependencies": {
    "@jest/test-sequencer": "^29.7.0",
    "@types/config": "^3.3.2",
    "@types/consul": "^0.40.2",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "@types/supertest": "^2.0.15",
    "af-consul": "^3.0.33",
    "eslint-config-af-22": "^1.0.13",
    "jest": "^29.7.0",
    "shx": "^0.3.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "np": {
    "yarn": false,
    "contents": "."
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/cjs/src",
    "dist/esm",
    "dist/types",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bazilio-san/af-tools-ts.git"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
